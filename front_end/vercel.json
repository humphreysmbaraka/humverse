{
  
    "redirects": [
      {
        "source": "/(.*)",
        "has": [
          { "type": "host", "value": "www.humverse.dev" }
        ],
        "destination": "https://humverse.dev/$1",
        "permanent": true
      },
      {
        "source": "/(.*)",
        "has": [
          { "type": "host", "value": "humverse.vercel.app" }
        ],
        "destination": "https://humverse.dev/$1",
        "permanent": true
      }
    ],
   
  

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
